.l-search-bar{
  background: #ef5b45;
  padding: 20rpx;
}
.l-search-bar .l-flex-hvc{
  background: #fff;
  padding: 15rpx 30rpx;
  border-radius: 15rpx;
}

.l-search-history{
  background: #fff;
  height: 100%;
  padding: 30rpx;
  box-sizing: border-box;
}

.l-search-history ._list text{
  float: left;
  background: #eee; 
  border-radius: 50rpx;
  padding: 10rpx 30rpx;
  font-size: 11px;
  margin: 20rpx 20rpx 0 0;
  line-height: 1.4;
}